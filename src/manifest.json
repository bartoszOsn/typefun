{
	"manifest_version": 3,
	"version": "1.0",
	"name": "browser Repl typescript",
	"devtools_page": "html/devTools.html",
	"permissions": [
		"storage",
		"tabs"
	],
	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"js": ["../node_modules/webextension-polyfill/dist/browser-polyfill.min.js", "entry/shimConsoleContentScript.ts"],
			"run_at": "document_end"
		}
	],
	"web_accessible_resources": [
		{
			"matches": ["*://*/*"],
			"resources": ["entry/shimConsolePageScript.ts"]
		}
	]
}