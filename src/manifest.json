{
	"manifest_version": 3,
	"version": "1.0",
	"name": "browser Repl typescript",
	"devtools_page": "pages/devtools/devTools.html",
	"permissions": [
		"storage",
		"tabs"
	],
	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"js": ["feature/devtools-console/shimConsoleContentScript.ts"],
			"run_at": "document_end"
		}
	],
	"web_accessible_resources": [
		{
			"matches": ["*://*/*"],
			"resources": ["feature/devtools-console/shimConsolePageScript.ts"]
		}
	]
}