{
	"manifest_version": 3,
	"version": "1.0",
	"name": "browser Repl typescript",
	"devtools_page": "devTools.html",
	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"js": ["browser-polyfill.min.js", "shimConsoleContentScript.js"],
			"run_at": "document_end"
		}
	],
	"web_accessible_resources": [
		{
			"matches": ["*://*/*"],
			"resources": ["shimConsolePageScript.js"]
		}
	]
}